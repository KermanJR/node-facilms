{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "***/*.css",
    "*.png"
, "src/app/theme/screens/SearchPage/Components/FilterSection", "src/app/content/site/screens/HomeScreen/Components/GoogleLoginButton/index.js", "src/app/api/encryptPagSeguro.js"  ],
  "exclude": [
    "node_modules"
  ]
}
